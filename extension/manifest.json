{
    "name": "ABUS Data Collector",
    "version": "0.1",
    "icons": {
        "16": "/img/icons/icon_16.png",
        "48": "/img/icons/icon_48.png",
        "128": "/img/icons/icon_128.png"
    },

    "description": "Yay, I'm so useless.",
    "permissions": [
        "geolocation"
    ],

    "content_scripts": [
        {
            "matches": ["https://ea.azure.cn/*"],
            "js": [
                    "/js/external/jquery.min.js",
                    "/js/content_script.js"
                ],
                "run_at": "document_start"
        }
    ],

    "manifest_version": 2
}
